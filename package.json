{
  "name": "ponymap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "db:migrate": "dotenv -e .env.local -- npx prisma migrate dev --schema=./src/prisma/schema.prisma",
    "db:push": "dotenv -e .env.local -- npx prisma db push --schema=./src/prisma/schema.prisma",
    "db:pull": "dotenv -e .env.local -- npx prisma db pull --schema=./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.15.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv-cli": "^7.4.2",
    "next": "14.2.4",
    "next-auth": "^4.24.7",
    "prisma": "^5.15.1",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
